  "query": "\n            SELECT c.id, c.name, c.description, c.website, c.phone, c.email, c.address, c.logo_url, c.timezone, uc.role, uc.is_primary\n            FROM companies c\n            JOIN user_company uc ON c.id = uc.company_id\n            WHERE uc.user_id = $1\n            ORDER BY uc.is_primary DESC, c.name ASC\n            ",
